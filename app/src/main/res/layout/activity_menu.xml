<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".menu"
    android:background="@drawable/gym1_login">

    <Button
        android:id="@+id/button17"
        android:layout_width="300dp"
        android:layout_height="53dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/round_button"
        android:text="LOGOUT"
        android:textColor="@color/colorAccent"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_words_dark" />

    <ImageButton
        android:id="@+id/imgsign"
        android:layout_width="119dp"
        android:layout_height="119dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/signgym"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="273dp" />

    <ImageButton
        android:id="@+id/imgsession"
        android:layout_width="119dp"
        android:layout_height="119dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/session"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="441dp" />

    <ImageButton
        android:id="@+id/imgstore"
        android:layout_width="119dp"
        android:layout_height="119dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/store"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="441dp" />

    <ImageButton
        android:id="@+id/imgnutrition"
        android:layout_width="119dp"
        android:layout_height="119dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/nutrition"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="273dp" />

</androidx.constraintlayout.widget.ConstraintLayout>